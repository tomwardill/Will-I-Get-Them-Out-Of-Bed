@{
    ViewBag.Title = "MultipleLookup";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <h1>There are multiple cities with that name, here are the results for all of them</h1>
</div>

@foreach (var result in ViewBag.MultipleResults)
{
    <div class="row">
        <h2>@result.Key.StandardName</h2>
    </div>
    
    if (result.Value)
     {

         <div class="hero-unit">
             <h1 id="answer">YES</h1>
         </div>

     }
     else
     {
         <div class="hero-unit">
             <h1 id="answer">MAYBE NOT?</h1>
         </div>
     }

    <div class="row">
        <div class="span-one-half">
            <p>It is currently: @result.Key.Now.DateTimeLocal.ToString()</p>
        </div>
    </div>
}
